{
  "ui": "stream",
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["DATABASE_URL", "API_URL", "AWS_REGION", "AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY", "EMAIL_DOMAIN", "PLUNK_API_KEY"],
  "globalPassThroughEnv": ["NODE_ENV", "npm_lifecycle_event"],
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts", ".expo/**", ".output/**", "dist/**"]
    },
    "dev": {
      "cache": false
    },
    "push": {
      "cache": false
    },
    "studio": {
      "cache": false
    },
    "format": {
      "outputLogs": "new-only",
      "outputs": ["node_modules/.cache/.prettiercache"]
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
