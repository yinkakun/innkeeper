{
  "name": "@innkeeper/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": "./src/index.ts",
  "scripts": {
    "build": "yarn with-env tsc",
    "lint": "eslint",
    "dev": "yarn with-env tsc --watch",
    "with-env": "dotenv -e ../../.env --",
    "clean": "rm -rf .turbo node_modules",
    "push": "yarn with-env drizzle-kit push",
    "seed": "yarn with-env tsx ./src/seed.ts",
    "studio": "yarn with-env drizzle-kit studio",
    "migrate": "yarn with-env drizzle-kit migrate",
    "generate": "yarn with-env drizzle-kit generate",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "format": "prettier --check . --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1"
  },
  "devDependencies": {
    "@libsql/sqlite3": "^0.3.1",
    "@types/chance": "^1.1.6",
    "chance": "^1.1.12",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.22.7",
    "eslint": "^9.5.0",
    "eslint-config-custom": "*",
    "prettier": "^3.3.1",
    "prettier-config": "*",
    "tsconfig": "*",
    "tsx": "^4.16.2",
    "typescript": "^5.5.2"
  },
  "prettier": "prettier-config"
}
